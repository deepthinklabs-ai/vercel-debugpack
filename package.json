{
  "name": "vercel-debugpack",
  "version": "0.1.4",
  "description": "On-demand debug bundle generation for Vercel + Chrome",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "debugpack": "./bin/debugpack.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.mjs",
      "require": "./dist/browser/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "debugpack": "node bin/debugpack.js",
    "test": "node --import tsx --test test/*.test.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "vercel",
    "debug",
    "logging",
    "chrome",
    "bundle",
    "staging"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deepthinklabs-ai/vercel-debugpack"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
